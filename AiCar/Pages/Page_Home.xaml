                    <Label.Background>
                        <ImageBrush ImageSource="/images/exit.png"/>
                    </Label.Background>
                </Label>

                Button x:Name="menu_close" Content="退出" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="34" Click="Button_Click" Height="45" Width="143" Margin="5,5,5,0"/-->

